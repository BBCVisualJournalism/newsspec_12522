<div id="ns_employmentResultsHolder" class="ns_employmentResultsHolder ns_hideMe">
	<h3><%= employmentDescrip %></h3>
	<div class="ns_employmentResultsBarHolder">
		<div class="ns_employmentResultsFemaleBar">
			<div class="ns_employmentBarFiguresHolder">
				<div id="ns_employmentResultsFemaleBar" class="ns_employmentBarFiguresHolderAbs">
					<span id="ns_employmentResultsFemaleBarVal" class="ns_employmentResultsVal"></span>
					<span id="ns_employmentResultsFemaleBarLabel" class="ns_employmentResultsLabel"><%= women %></span>
				</div>
			</div>
		</div>
		<div class="ns_employmentResultsMaleBar">
			<div class="ns_employmentBarFiguresHolder">
				<div id="ns_employmentResultsMaleBar" class="ns_employmentBarFiguresHolderAbs">
					<span id="ns_employmentResultsMaleBarVal" class="ns_employmentResultsVal"></span>
					<span id="ns_employmentResultsMaleBarLabel" class="ns_employmentResultsLabel"><%= men %></span>
				</div>
			</div>
		</div>
	</div>
	<!-- share button -->
	<div class="ns_shareHolder" id="ns_employmentShare">
		<button class="ns_shareButton"><%= share %></button>
		<div class="ns_shareOverlayAbsHolder">
			<div class="ns_shareOverlay ns_hideMe">
				<h3><%= share %></h3>
				<a href="#" class="ns_shareOverlayClose"></a>
				<ul>
					<li>
						<a href="#" target="_blank" data-window-width="626" data-window-height="236" data-network="email" tabindex="1" class="s-b-p-a istats-notrack email__share-button">
							<img src="<%= pathStatic %>/<%= vocab_dir %>/img/share_overlay_email_btn.png">
							<span><%= email %></span>
						</a>
					</li>
					<li>
						<a href="#" target="_blank" data-window-width="626" data-window-height="236" data-network="facebook" tabindex="1" class="s-b-p-a istats-notrack facebook__share-button">
							<img src="<%= pathStatic %>/<%= vocab_dir %>/img/share_overlay_facebook_btn.png">
							<span><%= facebook %></span>
						</a>
					</li>
					<li>
						<a href="#" target="_blank" data-window-width="626" data-window-height="236" data-network="twitter" tabindex="1" class="s-b-p-a istats-notrack twitter__share-button">
							<img src="<%= pathStatic %>/<%= vocab_dir %>/img/share_overlay_twitter_btn.png">
							<span><%= twitter %></span>
						</a>
					</li>
				</ul>
			</div>
		</div>
	</div>
	<!-- share button END -->
	<p><%= employmentFooter %></p>
</div>
<div class="ns_sectionSeperator ns_hideMe" id="ns_employmentResultsSeperator"></div>